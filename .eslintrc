{
  "extends": "airbnb-base/legacy",
  "env": {
	"browser": true,
	"node": true,
	"es6": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
	"ecmaVersion": 6,
	"sourceType": "module",
	"ecmaFeatures": {
	  "experimentalObjectRestSpread": true
	}
  },
  "rules": {
	"no-console": 0,
	"no-tabs": 0,
	"padded-blocks": 0,
	"indent": ["error", "tab"],
	"class-methods-use-this": 0,
	"no-underscore-dangle": 0,
	"no-shadow": 0,
	"no-restricted-syntax": 0,
	"no-await-in-loop": 0,
	"no-new": 0
  }
}
